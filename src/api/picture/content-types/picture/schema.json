    "Country": {
      "type": "string"
    },
    "reviews_ids": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "picture"
    },
    "favorite": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::favorite.favorite",
